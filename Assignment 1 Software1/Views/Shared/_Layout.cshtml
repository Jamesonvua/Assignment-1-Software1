<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - FAST Equipment Tool</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
    <div class="container">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="/">FAST Equipment Tool</a>
            <div class="collapse navbar-collapse">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Request" asp-action="AllEquipment">All Equipment Listing</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Request" asp-action="AvailableEquipment">Available Equipment</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Request" asp-action="RequestForm">Request Form</a>
                    </li>
                </ul>

            </div>
        </nav>

        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
</body>
</html>
